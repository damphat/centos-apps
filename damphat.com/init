CENTOS_APPS='/root/centos-apps'

$CENTOS_APPS/nginx/install.sh
$CENTOS_APPS/nodejs/install.sh


#restore damphat.com
mkdir /myprojects
mkdir /myprojects/damphat.com

git clone https://damphat@github.com/damphat/damphat.git /myprojects/damphat.com
$(cd /myprojects/damphat.com && npm install)

npm install -g supervisor
cp -rf /etc /

start damphat.com


